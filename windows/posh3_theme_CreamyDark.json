{
  "final_space": true,
  "console_title": false,
  "console_title_style": "",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "horizontal_offset": 0,
      "vertical_offset": 0,
      "segments": [
        {
            "type": "session",
            "style": "powerline",
            "foreground": "#b39286",
            "background": "#5c5a58",
            "properties": {
              "prefix": "  ",
              "host_color": "#aaaaaa",
              "user_info_separator": "-"
            }
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#1F1E1D",
          "background": "#888",
          "properties": {
            "prefix": " ",
            "style": "agnoster",
            "home_icon": "~",
            "folder_icon" : "..."
          }
        },
        {
          "type": "spotify",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#baaf97",
          "background": "#52735e",
          "properties": {
            "playing_icon": "\uF9C6"
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#fffb38",
          "properties": {
            "display_stash_count": true,
            "display_upstream_icon": true
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#193549",
          "background": "#ffff66",
          "properties": {
            "root_icon": "\uF0E7ADMIN"
          }
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#999",
          "background": "#5c5a58",
          "properties": {
            "always_enabled": true,
            "style": "amarillo",
            "prefix": " <#777>\ufbab</> "
          }
        }
      ]
    }
  ]
}
